		"ts-node-maintained": "^10.9.5",
		"typescript": "~5.8"
	},
	"dependencies": {
		"@adonisjs/ally": "^5.1.0",
		"@adonisjs/auth": "^9.4.0",
		"@adonisjs/core": "^6.18.0",
