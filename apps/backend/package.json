{
	"name": "@klaz/backend",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"license": "UNLICENSED",
	"scripts": {
		"start": "node bin/server.js",
		"build": "node ace build",
		"dev": "node ace serve --hmr",
