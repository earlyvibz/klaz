{
	"name": "@klaz/backend",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"license": "UNLICENSED",
	"files": [
		"build/",
		".adonisjs/",
		"adonisrc.minimal.ts",
		"package.json"
	],
	"scripts": {
		"local": "NODE_ENV=local node ace serve --hmr",
		"start": "NODE_ENV=production node bin/server.js",
		"start:staging": "NODE_ENV=staging node bin/server.js",
		"start:dev": "NODE_ENV=development node bin/server.js",
		"start:local": "NODE_ENV=local node bin/server.js",
		"build": "node ace build",
		"dev": "NODE_ENV=development node ace serve --hmr",
		"test": "NODE_ENV=test node ace test",
		"test:coverage": "NODE_ENV=test c8 node ace test",
		"typecheck": "tsc --noEmit",
		"seed:local": "NODE_ENV=local node --env-file=.env.local ace db:seed",
		"seed:dev": "NODE_ENV=development node --env-file=.env.development ace db:seed"
	},
	"imports": {
		"#controllers/*": "./app/controllers/*.js",
		"#exceptions/*": "./app/exceptions/*.js",
		"#models/*": "./app/models/*.js",
		"#mails/*": "./app/mails/*.js",
		"#services/*": "./app/services/*.js",
		"#listeners/*": "./app/listeners/*.js",
		"#events/*": "./app/events/*.js",
		"#middleware/*": "./app/middleware/*.js",
		"#validators/*": "./app/validators/*.js",
		"#providers/*": "./providers/*.js",
		"#policies/*": "./app/policies/*.js",
		"#abilities/*": "./app/abilities/*.js",
		"#database/*": "./database/*.js",
		"#start/*": "./start/*.js",
		"#tests/*": "./tests/*.js",
		"#config/*": "./config/*.js",
		"#types/*": "./app/types/*.js",
		"#dtos/*": "./app/dtos/*.js"
	},
	"exports": {
		"./api": "./.adonisjs/index.ts"
	},
	"devDependencies": {
		"@adonisjs/assembler": "^7.8.2",
		"@adonisjs/tsconfig": "^1.4.0",
		"@japa/api-client": "^3.1.0",
		"@japa/assert": "^4.0.1",
		"@japa/plugin-adonisjs": "^4.0.0",
		"@japa/runner": "^4.2.0",
		"@swc/core": "1.11.24",
		"@tuyau/utils": "^0.0.9",
		"@types/luxon": "^3.6.2",
		"@types/node": "^22.15.18",
		"c8": "^10.1.3",
		"hot-hook": "^0.4.0",
		"ts-node-maintained": "^10.9.5",
		"typescript": "~5.8"
	},
	"dependencies": {
		"@adocasts.com/dto": "^0.0.10",
		"@adonisjs/ally": "^5.1.0",
		"@adonisjs/auth": "^9.4.0",
		"@adonisjs/core": "^6.18.0",
		"@adonisjs/cors": "^2.2.1",
		"@adonisjs/drive": "^3.4.1",
		"@adonisjs/i18n": "^2.2.2",
		"@adonisjs/limiter": "^2.4.0",
		"@adonisjs/lucid": "^21.6.1",
		"@adonisjs/mail": "^9.2.2",
		"@adonisjs/session": "^7.5.1",
		"@aws-sdk/client-s3": "^3.864.0",
		"@aws-sdk/s3-request-presigner": "^3.864.0",
		"@rlanz/sentry": "^0.3.3",
		"@tuyau/core": "^0.4.2",
		"@vinejs/vine": "^3.0.1",
		"luxon": "^3.6.1",
		"pg": "^8.16.3",
		"pino-pretty": "^13.0.0",
		"reflect-metadata": "^0.2.2",
		"uuid": "^11.1.0"
	},
	"hotHook": {
		"boundaries": [
			"./app/controllers/**/*.ts",
			"./app/middleware/*.ts"
		]
	}
}
